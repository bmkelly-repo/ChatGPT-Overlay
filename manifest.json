{
  "manifest_version": 3,
  "name": "Prompt Manager",
  "version": "1.0",
  "description": "An overlay for managing prompts and presets for ChatGPT.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon_opt1_16.png",
      "48": "icons/icon_opt1_48.png",
      "128": "icons/icon_opt1_128.png"
    }
  }
}

